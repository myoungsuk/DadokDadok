<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
		PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<!-- SQL문 정의할 때 VO표현을 간단하게 하고 싶은 경우. 별명을 지어서 사용 -->
		<typeAlias type="com.multi.mini6.bookpage.vo.BookVO" alias="bookVO"></typeAlias>
		<typeAlias type="com.multi.mini6.bookpage.vo.PageVO" alias="pageVO"></typeAlias>
		<typeAlias type="com.multi.mini6.bookpage.vo.MovieVO" alias="movieVO"></typeAlias>
		<typeAlias type="com.multi.mini6.loginpage.vo.AuthorityVO" alias="authorityVO"></typeAlias>
		<typeAlias type="com.multi.mini6.loginpage.vo.MemberVO" alias="memberVO"></typeAlias>
		<typeAlias type="com.multi.mini6.reviewboard.vo.ReviewVO" alias="reviewVO"></typeAlias>
		<typeAlias type="com.multi.mini6.loginpage.vo.MemberDeleteReasonVO" alias="memberDeleteReasonVO"></typeAlias>
		<typeAlias type="com.multi.mini6.reviewboard.vo.ReviewCommentVO" alias="reviewcommentVO"></typeAlias>
		<typeAlias type="com.multi.mini6.reviewboard.vo.PageVo" alias="pageVO2"></typeAlias>
		<typeAlias type="com.multi.mini6.reviewboard.vo.ReviewAttachVO" alias="reviewattachVO"></typeAlias>
		<typeAlias type="com.multi.mini6.noticeboard.vo.NoticeBoardVO" alias="noticeboardVO"></typeAlias>
		<typeAlias type="com.multi.mini6.noticeboard.vo.NoticeBoardPageVO" alias="noticeboardpageVO"></typeAlias>
		<typeAlias type="com.multi.mini6.loginpage.vo.GuestBookVO" alias="guestBookVO"></typeAlias>
		<typeAlias type="com.multi.mini6.librarypage.vo.LibraryVO" alias="libraryVO"></typeAlias>
		<typeAlias type="com.multi.mini6.librarypage.vo.LibraryPageVO" alias="libraryPageVO"></typeAlias>
	</typeAliases>

	<mappers>
		<!-- SQL문 정의하는 파일들의 목록을 지정. -->
		<mapper resource="mapper/ReviewSQL2.xml" />
		<mapper resource="mapper/ReviewCommentSQL.xml" />
		<mapper resource="mapper/MemberSQL.xml" />
		<mapper resource="mapper/BookSQL.xml" />
		<mapper resource="mapper/MovieSQL.xml" />
		<mapper resource="mapper/NoticeBoardSQL.xml" />
		<mapper resource="mapper/GuestBookEntrySQL.xml" />
		<mapper resource="mapper/LibrarySQL.xml" />
	</mappers>
</configuration>




